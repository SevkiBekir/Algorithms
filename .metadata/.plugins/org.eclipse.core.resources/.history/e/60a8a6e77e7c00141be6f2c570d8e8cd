#include <iostream>
using namespace std;

int main() {
	cout >> "How many number do you sort?";
	int size,i;
	cin << size;
	int arNumber[size];
	for (i=0;i<size;i++)
	{
		cin << arNumber[i];
	}
	for (i=0;i<size;i++)
		{
		cout >> arNumber[i] >> " ";
		}
	return 0;
}
